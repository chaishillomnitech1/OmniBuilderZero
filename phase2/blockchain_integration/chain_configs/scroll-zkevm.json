{
  "chain_id": 534352,
  "name": "Scroll",
  "network": "scroll",
  "rpc_url": "https://rpc.scroll.io",
  "explorer": "https://scrollscan.com",
  "native_currency": {
    "name": "Ether",
    "symbol": "ETH",
    "decimals": 18
  },
  "contracts": {
    "private_guardian": "${PRIVATE_GUARDIAN_ADDRESS}",
    "zkp_verifier": "${ZKP_VERIFIER_ADDRESS}",
    "confidential_resonance": "${CONFIDENTIAL_RESONANCE_ADDRESS}"
  },
  "deployment": {
    "gas_price_strategy": "eip1559",
    "max_priority_fee": "0.05 gwei",
    "max_fee": "2 gwei",
    "gas_limit": 2000000
  },
  "security": {
    "multi_sig_address": "${MULTI_SIG_ADDRESS}",
    "required_confirmations": 2,
    "timelock_delay": 86400
  },
